{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=.;Database=ServiceMarketplace;Trusted_Connection=True;TrustServerCertificate=True"
  },
  "RequestLocalizationSettings": {
    "DefaultRequestCulture": "en-US",
    "SupportedCultures": {
      "EnCode": "en-US",
      "BgCode": "bg-BG"
    }
  },
  "SwaggerSettings": {
    "Title": "Web Application",
    "Version": "v1",
    "SecurityDefinitionType": "JWT",
    "Description": "Add valid JWT token",
    "SecurityDefinitionName": "Authorization",
    "BearerFormat": "JWT",
    "Scheme": "Bearer"
  },
  "AllowedHosts": "*"
}
